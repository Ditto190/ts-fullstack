{
  "name": "@adaptiveworx/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint:check": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "type-coverage": "type-coverage --at-least 85 --strict --detail --ignore-files '**/*.test.{ts,tsx}'"
  },
  "dependencies": {
    "@adaptiveworx/shared": "workspace:*",
    "@adaptiveworx/ui": "workspace:*",
    "@tanstack/react-query": "^5.62.14",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/router-devtools": "^1.132.51",
    "@tanstack/router-vite-plugin": "^1.132.51",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.2",
    "vite": "^6.0.7",
    "vitest": "^3.2.4"
  }
}
